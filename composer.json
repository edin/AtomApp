{
    "name": "atom/app",
    "type": "project",
    "description": "Atom application template",
    "keywords": [
        "framework",
        "Atom"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/edin/Atom.git"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "atom/framework": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}